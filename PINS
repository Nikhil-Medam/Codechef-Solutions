#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        cout<<"1 1";
        for(int i=0;i<n/2;i++)
            cout<<"0";
        cout<<endl;    
    }
    return 0;
}   